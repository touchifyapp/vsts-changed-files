{
  "manifestVersion": 1,
  "id": "vsts-changed-files",
  "version": "2.0.0",
  "name": "Changed files",
  "description": "Pipeline task to get changed files and run tasks according to those changes.",
  "publisher": "Touchify",
  "public": true,
  "icons": {
    "default": "images/icon.png",
    "large": "images/icon-large.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "screenshots": [
    {
      "path": "images/screen-simple.jpg"
    },
    {
      "path": "images/screen-advanced.jpg"
    },
    {
      "path": "images/screen-multi.jpg"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/touchifyapp/vsts-changed-files"
    },
    "support": {
      "uri": "https://github.com/touchifyapp/vsts-changed-files/issues"
    },
    "issues": {
      "uri": "https://github.com/touchifyapp/vsts-changed-files/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/touchifyapp/vsts-changed-files"
  },
  "badges": [
    {
      "href": "https://github.com/touchifyapp/vsts-changed-files/actions",
      "uri": "https://github.com/touchifyapp/vsts-changed-files/workflows/CI/badge.svg?event=push",
      "description": "Github Actions build for the project"
    }
  ],
  "branding": {
    "color": "#f2f2f2",
    "theme": "light"
  },
  "tags": [
    "utility",
    "changed",
    "files",
    "changeset",
    "condition",
    "variable",
    "output",
    "optimization",
    "performance"
  ],
  "files": [
    {
      "path": "changed-files"
    }
  ],
  "contributions": [
    {
      "id": "vsts-changed-files",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "changed-files"
      }
    }
  ]
}